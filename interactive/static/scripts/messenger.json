{
  "interval": 500,
  "lines": [
    "1 new",
    "2 new",
    "3 new",
    "1 call Controller activate {\"callsign\":\"Messenger\"}",
    "1 on Messenger roomupdate",
    "2 call Controller activate {\"callsign\":\"Messenger\"}",
    "2 on Messenger roomupdate",
    "3 call Controller activate {\"callsign\":\"Messenger\"}",
    "3 on Messenger roomupdate",
    "1 call Messenger create {\"room\":\"Test\",\"urlRegex\":\"*://*:*\"}",
    "1 call Messenger join {\"user\":\"Host\",\"room\":\"Test\"}",
    "1 on Messenger {\"event\":\"userupdate\",\"prefix\":\"<<<1.Messenger.join.roomid>>>\"}",
    "1 on Messenger {\"event\":\"message\",\"prefix\":\"<<<1.Messenger.join.roomid>>>\"}",
    "2 call Messenger join {\"user\":\"Guest\",\"room\":\"Test\"}",
    "2 on Messenger {\"event\":\"userupdate\",\"prefix\":\"<<<2.Messenger.join.roomid>>>\"}",
    "2 on Messenger {\"event\":\"message\",\"prefix\":\"<<<2.Messenger.join.roomid>>>\"}",
    "1 call Messenger send {\"message\":\"Hi, <<<1.Messenger.userupdate.user>>>\",\"roomid\":\"<<<1.Messenger.join.roomid>>>\"}",
    "2 call Messenger send {\"message\":\"Got '<<<2.Messenger.message.message>>>'\",\"roomid\":\"<<<2.Messenger.join.roomid>>>\"}",
    "1 call Messenger leave {\"roomid\":\"<<<1.Messenger.join.roomid>>>\"}",
    "2 call Messenger leave {\"roomid\":\"<<<2.Messenger.join.roomid>>>\"}",
    "2 off Messenger {\"event\":\"userupdate\",\"prefix\":\"<<<2.Messenger.join.roomid>>>\"}",
    "2 off Messenger {\"event\":\"message\",\"prefix\":\"<<<2.Messenger.join.roomid>>>\"}",
    "1 off Messenger {\"event\":\"userupdate\",\"prefix\":\"<<<1.Messenger.join.roomid>>>\"}",
    "1 off Messenger {\"event\":\"message\",\"prefix\":\"<<<1.Messenger.join.roomid>>>\"}",
    "1 off Messenger roomupdate",
    "2 off Messenger roomupdate",
    "3 off Messenger roomupdate",
    "1 delete",
    "2 delete",
    "3 delete"
  ]
}
